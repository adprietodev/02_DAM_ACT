<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="es.florida.main">
	<class name="Procesador" table="procesadores">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="modelo" type="string" column="modelo" />
		<property name="precio" type="int" column="precio" />
		<property name="precioOferta" type="int" column="precioOferta" />
		<property name="disponibilidad" type="string" column="disponibilidad" />
	</class>
</hibernate-mapping>